{
  "name": "ad-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "migrate:postgres": "dotenv -e .env.local -- pnpm dlx prisma migrate deploy",
    "migrate-dev:postgres": "dotenv -e .env.local -- pnpm dlx prisma migrate dev --name",
    "seed:postgres": "dotenv -e .env.local -- pnpm dlx prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.1",
    "@prisma/client": "^5.2.0",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "classnames": "^2.3.2",
    "dotenv": "^16.3.1",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "moment": "^2.29.4",
    "next": "13.4.19",
    "next-cloudinary": "^4.21.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.1",
    "typescript": "5.2.2",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "prisma": "^5.2.0",
    "sass": "^1.66.1",
    "ts-node": "^10.9.1"
  }
}
